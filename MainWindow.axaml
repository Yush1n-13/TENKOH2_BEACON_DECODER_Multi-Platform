                                                    <TextBlock x:Name="txtBatteryVoltage" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" V" Width="30"/>
                                            </StackPanel>
                                        </StackPanel>

                                        <!-- Second Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="1">
                                            <!-- Battery Temperature -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
