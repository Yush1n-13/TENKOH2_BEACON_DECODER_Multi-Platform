<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p1="clr-namespace:Material.Styles.Assists;assembly=Material.Styles"
        xmlns:controls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
        xmlns:icons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
        

        x:Class="TENKOH2_BEACON_DECODER_Multi_Platform.MainWindow"
        Width="1070"
        Height="795"
        CanResize="True"
        Title="TENKOH2_BEACON_DECODER DEMO"
        Icon="Images\TK2_icon.ico">

    <StackPanel Orientation="Vertical">

        <controls:ColorZone Height="30" Mode="PrimaryMid">
            <StackPanel Orientation="Horizontal">
                <TextBlock Text="  TEN-KOH2 BEACON DECODER " VerticalAlignment="Center" FontSize="20" FontWeight="Bold"/>
            </StackPanel>
        </controls:ColorZone>

        <!-- Column 1: Existing GUI -->
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" >  
            <StackPanel Orientation="Vertical">
                <TextBox 
                    x:Name="InputTextBox" 
                    Height="60" 
                    HorizontalAlignment="Left"
                    VerticalContentAlignment="Bottom"
                    UseFloatingWatermark="True" 
                    p1:TextFieldAssist.Label="Insert Beacon String"
                    Width="330" 
                    Margin="10,5,10,10"
                    FontSize="16"/>

                <controls:Card
                    x:Name="TimestampCard"
                    Width="330" 
                    Height="50" 
                    HorizontalAlignment="Left" 
                    Margin="10,0,10,10">
                
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="TimeStamp" FontWeight="Bold" FontSize="16" Width="100" Margin="0,5,0,5"/>
                        <TextBox x:Name="TimestampTextBox" IsReadOnly="True" FontSize="16"/>
                    </StackPanel>
                </controls:Card>
            </StackPanel>

            <StackPanel Orientation="Vertical">
                <Button 
                    x:Name="NUDecodeButton" 
                    Click="NUDecodeButton_Click" 
                    Content="Decode" 
                    Width="90" 
                    FontSize="14"
                    Margin="10, 20, 10, 5"/>
                <Button 
                    Click="ClearButton_Click" 
                    Content="Clear" 
                    Width="90" 
                    FontSize="14"
                    Margin="10, 0, 10, 10"/>
                <TextBlock 
                    Text="JS1YKI"
                    FontWeight="Bold"
                    FontStyle="Italic"
                    FontSize="22"
                    HorizontalAlignment="Center"/>
            </StackPanel>

            <controls:Card
                x:Name="InputControllCard"
                Margin="10"
                Padding="0"
                Width="160">
                <StackPanel Orientation="Vertical">

                    <controls:ColorZone Height="30" Mode="PrimaryMid">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text=" Input Mode" VerticalAlignment="Center" FontSize="16" FontWeight="Bold"/>
                            <ToggleButton Content="{icons:MaterialIconExt Cog}" Margin="20,0,0,0" Click="SettingsButton_Click" />
                        </StackPanel>
                    </controls:ColorZone>

                    <RadioButton 
                        x:Name="ManualInputRadio" 
                        Content="Manual Input" 
                        FontSize="14" 
                        IsChecked="True" 
                        GroupName="InputMode"
                        Checked="RadioButton_Checked"
                        Margin="5,10,5,0"/>

                    <RadioButton 
                        x:Name="AutomaticInputRadio" 
                        Content="Automatic Input"
                        FontSize="14" 
                        GroupName="InputMode"
                        Checked="RadioButton_Checked"
                        Margin="5"/>
                </StackPanel>
            </controls:Card>

            <Image 
                HorizontalAlignment="Left"  
                Source="Images\TK2.png" 
                Width="110" 
                Margin="10"/>
            <!--Image 
                HorizontalAlignment="Left"  
                Source="Images\TK2_Satellite.png" 
                Height="110"
                Margin="10"/-->

        </StackPanel>

        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top">
            <TabControl x:Name="MainTabControl"  HorizontalAlignment="Left">
                <TabControl.Items>
                    <TabItem Header="Nominal Mode">
                        <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
                            <controls:Card
                                x:Name="Card1" 
                                Width="440" 
                                Height="150" 
                                HorizontalAlignment="Left"
                                Margin="10,10,10,0">

                                <StackPanel>
                                    <TextBlock Text="Telemetry" FontWeight="Bold" FontSize="16" Margin="0,0,0,5"/>

                                        <!-- Opreation Mode   -->
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                            <TextBlock Text="Opreation Mode: " Width="150"/>
                                            <TextBlock x:Name="txtOperationMode" FontStyle="Italic" Width="200"/>
                                        </StackPanel>

                                        <!-- EPS Controller Opreation Mode   -->
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                            <TextBlock Text="EPS Controller Status: " Width="150"/>
                                            <TextBlock x:Name="txtEPSControllerStatus" FontStyle="Italic" Width="200"/>
                                        </StackPanel>

                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- First Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="0">
                                            <!-- Battery Current -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="Battery Current: " Width="150"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtBatteryCurrent" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" A" Width="30"/>
                                            </StackPanel>
                                            <!-- Battery Status -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="Charge/Discharge:" Width="130"/>
                                                <TextBlock x:Name="txtBatteryStatus" FontStyle="Italic" Width="90"/>
                                            </StackPanel>
                                            <!-- Battery Voltage -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="Battery Voltage: " Width="150"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtBatteryVoltage" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" V" Width="30"/>
                                            </StackPanel>
                                        </StackPanel>

                                        <!-- Second Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="1">
                                            <!-- Battery Temperature -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                <TextBlock Text="Battery Temperature: " Width="150"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtBatteryTemperature" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" °C" Width="30"/>
                                            </StackPanel>
                                            <!-- WDU Temperature -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                <TextBlock Text="WDU Temperature: " Width="150"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtWDUTemperature" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" °C" Width="30"/>
                                            </StackPanel>
                                            <!-- MCU Temperature -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                <TextBlock Text="MCU Temperature: " Width="150"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtMCUTemperature" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" °C" Width="30"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </Grid>
                                </StackPanel>
                            </controls:Card>

                            <controls:Card
                                x:Name="Card2" 
                                Width="440" 
                                Height="180" 
                                HorizontalAlignment="Left" 
                                Margin="10,10,10,0">

                                <StackPanel>
                                    <TextBlock Text="Satellite Status" FontWeight="Bold" FontSize="16" Margin="0,0,0,5"/>

                                    <!-- GPIO Expander ID -->
                                    <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                        <TextBlock Text="GPIO Expander ID: " Width="150"/>
                                        <TextBlock x:Name="txtGPIOExpander" FontStyle="Italic" Width="70"/>
                                        <TextBlock Width="30"/>
                                    </StackPanel>

                                    <Grid>    
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- First Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="0" Margin="0,0,15,0">
                                            <!-- 5V_CAM -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="5V_CAM:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator5VCAM" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt5VCAM" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 5V_PL -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="5V_PL:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator5VPL" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt5VPL" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 5V_NUM -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="5V_NUM:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator5VNUM" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt5VNUM" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 3V3_JAMSAT -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="3V3_JAMSAT:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator3V3JAMSAT" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt3V3JAMSAT" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 3V3_ADCS -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="3V3_ADCS:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator3V3ADCS" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt3V3ADCS" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                        </StackPanel>

                                        <!-- Second Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="1">
                                            <!-- 5V_OBC -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="5V_OBC:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator5VOBC" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt5VOBC" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 5V_ADCS -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="5V_ADCS:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator5VADCS" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt5VADCS" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 5V_COM -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="5V_COM:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator5VCOM" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt5VCOM" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 12V_ADCS -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="12V_ADCS:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator12VADCS" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt12VADCS" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            <!-- 12V_LIU -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="12V_LIU:" Width="130"/>
                                                <Ellipse x:Name="statusIndicator12VLIU" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txt12VLIU" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                        </StackPanel>
                                    </Grid>
                                </StackPanel>
                            </controls:Card>

                            <controls:Card
                                x:Name="Card7" 
                                Width="440" 
                                Height="205" 
                                HorizontalAlignment="Left" 
                                Margin="10">

                                <StackPanel>
                                    <TextBlock Text="Subsystem Interface Status" FontWeight="Bold" FontSize="16" Margin="0,0,0,5"/>

                                    <!-- System Check -->
                                    <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                        <TextBlock Text="System Check: " Width="120"/>
                                        <TextBlock x:Name="txtSystemCheck" FontStyle="Italic" Width="220"/>
                                        <TextBlock Width="30"/>
                                    </StackPanel>

                                    <Grid>    
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- First Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="0" Margin="0,0,15,0">

                                            <!-- I2C_RTC -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_RTC:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CRTC" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CRTC" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            
                                            <!-- I2C_MEM -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_MEM:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CMEM" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CMEM" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                            <!-- I2C_EPSC -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_EPSC:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CEPSC" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CEPSC" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                            <!-- I2C_COM -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_COM:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CCOM" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CCOM" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                            <!-- I2C_ANT -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_ANT:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CANT" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CANT" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                            <!-- I2C_IFPV -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_IFPV:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CIFPV" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CIFPV" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                        </StackPanel>

                                        <!-- Second Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="1">

                                            <!-- I2C_ADCS -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_ADCS:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CADCS" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CADCS" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            
                                            <!-- I2C_CAM -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_CAM:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CCAM" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CCAM" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            
                                            <!-- I2C_MATLIU -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_MATLIU:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CMATLIU" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CMATLIU" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>
                                            
                                            <!-- I2C_NU -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="I2C_NU:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorI2CNU" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtI2CNU" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                            <!-- UART_JAMSAT -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="UART_JAMSAT:" Width="110"/>
                                                <Ellipse x:Name="statusIndicatorUARTJAMSAT" Width="10" Height="10" Fill="Gray" VerticalAlignment="Center" Margin="5,0,5,0"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="60" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtUARTJAMSAT" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                            </StackPanel>

                                        </StackPanel>
                                    </Grid>
                                </StackPanel>
                            </controls:Card>
                        </StackPanel>
                    </TabItem>

                    <TabItem Header="JAMSAT Mission Mode">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                            <StackPanel Orientation="Vertical" HorizontalAlignment="Left">
                                <controls:Card
                                    x:Name="Card3" 
                                    Width="440" 
                                    Height="150" 
                                    HorizontalAlignment="Left"
                                    Margin="10,10,10,0">

                                    <StackPanel>
                                        <TextBlock Text="Telemetry" FontWeight="Bold" FontSize="16" Margin="0,0,0,5"/>

                                            <!-- Opreation Mode   -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                <TextBlock Text="Opreation Mode: " Width="150"/>
                                                <TextBlock x:Name="txtOperationModeJ" FontStyle="Italic" Width="200"/>
                                            </StackPanel>

                                            <!-- EPS Controller Opreation Mode   -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                <TextBlock Text="EPS Controller Status: " Width="150"/>
                                                <TextBlock x:Name="txtEPSControllerStatusJ" FontStyle="Italic" Width="200"/>
                                            </StackPanel>

                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <!-- First Column -->
                                            <StackPanel Orientation="Vertical" Grid.Column="0">
                                                <!-- Battery Current -->
                                                <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                    <TextBlock Text="Battery Current: " Width="150"/>
                                                    <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                        <TextBlock x:Name="txtBatteryCurrentJ" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <TextBlock Text=" A" Width="30"/>
                                                </StackPanel>
                                                <!-- Battery Status -->
                                                <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                    <TextBlock Text="Charge/Discharge: " Width="130"/>
                                                    <TextBlock x:Name="txtBatteryStatusJ" FontStyle="Italic" Width="90" />
                                                </StackPanel>
                                                <!-- Battery Voltage -->
                                                <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                    <TextBlock Text="Battery Voltage: " Width="150"/>
                                                    <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                        <TextBlock x:Name="txtBatteryVoltageJ" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <TextBlock Text=" V" Width="30"/>
                                                </StackPanel>
                                            </StackPanel>

                                            <!-- Second Column -->
                                            <StackPanel Orientation="Vertical" Grid.Column="1">
                                                <!-- Battery Temperature -->
                                                <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                    <TextBlock Text="Battery Temperature: " Width="150"/>
                                                    <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                        <TextBlock x:Name="txtBatteryTemperatureJ" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <TextBlock Text=" °C" Width="30"/>
                                                </StackPanel>
                                                <!-- WDU Temperature -->
                                                <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                                    <TextBlock Text="WDU Temperature: " Width="150"/>
                                                    <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                        <TextBlock x:Name="txtWDUTemperatureJ" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                    </Border>
                                                    <TextBlock Text=" °C" Width="30"/>
                                                </StackPanel>
                                                <!-- MCU Temperature -->
