<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:p1="clr-namespace:Material.Styles.Assists;assembly=Material.Styles"
        xmlns:controls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"

        x:Class="TENKOH2_BEACON_DECODER_Multi_Platform.MainWindow"
        Width="920"
        Height="770"
        CanResize="False"
        Title="TENKOH2_BEACON_DECODER_v1"
        Icon="Images\TK2_icon.ico">

    <StackPanel Orientation="Vertical">
        <!-- Column 1: Existing GUI -->
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" >  
            <StackPanel Orientation="Vertical">
                <TextBox 
                    x:Name="InputTextBox" 
                    Height="60" 
                    HorizontalAlignment="Left"
                    VerticalContentAlignment="Bottom"
                    UseFloatingWatermark="True" 
                    p1:TextFieldAssist.Label="Insert Beacon String"
                    Width="330" 
                    Margin="10"
                    FontSize="16"/>

                <controls:Card
                    x:Name="TimestampCard"
                    Width="330" 
                    Height="50" 
                    HorizontalAlignment="Left" 
                    Margin="10,0,10,10">
                
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Text="TimeStamp" FontWeight="Bold" FontSize="16" Width="100" Margin="0,5,0,5"/>
                        <TextBox x:Name="TimestampTextBox" IsReadOnly="True" FontSize="16"/>
                    </StackPanel>
                </controls:Card>
            </StackPanel>
            <StackPanel Orientation="Vertical">
                <Button 
                    x:Name="NUDecodeButton" 
                    Click="NUDecodeButton_Click" 
                    Content="Decode" 
                    Width="90" 
                    FontSize="14"
                    Margin="10, 20, 10, 5"/>
                <Button 
                    Click="ClearButton_Click" 
                    Content="Clear" 
                    Width="90" 
                    FontSize="14"
                    Margin="10, 0, 10, 10"/>
                <TextBlock 
                    Text="JS1YKI"
                    FontWeight="Bold"
                    FontStyle="Italic"
                    FontSize="22"
                    HorizontalAlignment="Center"/>
            </StackPanel>
            <Image 
                HorizontalAlignment="Left"  
                Source="Images\TK2.png" 
                Width="110" 
                Margin="10"/>
            <!--Image 
                HorizontalAlignment="Left"  
                Source="Images\TK2_Satellite.png" 
                Height="110"
                Margin="10"/-->
        </StackPanel>

        <TabControl x:Name="MainTabControl" Width="920" HorizontalAlignment="Left">
            <TabControl.Items>
                <TabItem Header="NUM">
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                        <StackPanel Orientation="Horizontal" Width="460" HorizontalAlignment="Left">
                            <controls:Card
                                x:Name="Card1" 
                                Width="440" 
                                Height="180" 
                                HorizontalAlignment="Left"
                                Margin="10">

                                <StackPanel>
                                    <TextBlock Text="Telemetry" FontWeight="Bold" FontSize="16" Margin="0,0,0,5"/>

                                        <!-- Opreation Mode   -->
                                        <StackPanel Orientation="Horizontal" Margin="0,0,0,5">
                                            <TextBlock Text="Opreation Mode: " Width="150"/>
                                            <TextBlock x:Name="txtOperationMode" FontStyle="Italic" Width="100"/>
                                        </StackPanel>

                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>

                                        <!-- First Column -->
                                        <StackPanel Orientation="Vertical" Grid.Column="0">
                                            <!-- Battery Current -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="Battery Current: " Width="150"/>
                                                <Border Background="#F5F5F5" BorderBrush="Black" BorderThickness="1" Width="40" VerticalAlignment="Center">
                                                    <TextBlock x:Name="txtBatteryCurrent" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                                </Border>
                                                <TextBlock Text=" A" Width="30"/>
                                            </StackPanel>
                                            <!-- Battery Status -->
                                            <StackPanel Orientation="Horizontal" Margin="0,0,5,5">
                                                <TextBlock Text="Charge/Discharge" Width="150"/>
                                                <TextBlock x:Name="txtBatteryStatus" FontStyle="Italic" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                            </StackPanel>
                                            <!-- Battery Voltage -->
